<%%= render 'administration/form_errors', resource: @<%= singular_table_name %> %>

<%%= simple_form_for @<%= singular_table_name %>,
    builder: AdminFormBuilder,
    defaults: { input_html: { class: :large } },
    alias_set: params[:alias_set] do |f| %>

  <table width='600' class='admin-view-30'>

    <%- attributes.each do |attribute| -%>
      <%%= f.<%= attribute.reference? ? :association : :input %> :<%= attribute.name %> %>
    <%- end -%>

  </table>

  <br>

  <%%= f.button :submit %>

<%% end %>
